{
    "build": {
      "builder": "NODE_JS",
      "nodeVersion": "18.x",
      "installCommand": "npm install",
      "buildCommand": "npm run build",
      "outputDirectory": "."
    },
    "deploy": {
      "startCommand": "npm start",
      "restartPolicy": {
        "type": "ALWAYS"
      },
      "healthcheckPath": "/",
      "healthcheckTimeout": 30
    }
  }